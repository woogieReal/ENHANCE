package com.sist.feb.category.cmn;

/**
 * @author 임하람
 *
 */
public class DTO {
	
	//화면에 "성공" or "실패" 메시지 전달
	
	private int msgFlag;
	private String msg;
	
	public DTO() { }//default 생성자
	

	public int getMsgFlag() {
		return msgFlag;
	}

	public void setMsgFlag(int msgFlag) {
		this.msgFlag = msgFlag;
	}

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}


	@Override
	public String toString() {
		return "DTO [msgFlag=" + msgFlag + ", msg=" + msg + ", toString()=" + super.toString() + "]";
	}
	
	
	
	

}//--class
