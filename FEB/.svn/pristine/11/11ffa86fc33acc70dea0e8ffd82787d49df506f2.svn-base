package com.sist.feb.category.domain;

import com.sist.feb.category.cmn.DTO;

/**
 * @author 임하람
 * 카테고리 구분 VO
 */
public class SearchVO extends DTO {
	private String div;//카테고리 구분
	
	public SearchVO() { }//default 생성자

	/**
	 * @param div
	 */
	public SearchVO(String div) {
		super();
		this.div = div;
	}

	public String getDiv() {
		return div;
	}

	public void setDiv(String div) {
		this.div = div;
	}

	
	
	@Override
	public String toString() {
		return "SearchVO [div=" + div + ", toString()=" + super.toString() + "]";
	}
	

}//--class
