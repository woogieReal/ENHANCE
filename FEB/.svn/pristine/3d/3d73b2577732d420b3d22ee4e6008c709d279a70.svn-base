@charset "UTF-8";
/*header css*/
.header{
    margin:0;
    text-decoration:none;
	background-color: #263343;
}

a{
	 text-decoration:none;
	 color:white;
}

/*//header css*/

/*common css*/
* {
    padding: 0px;
    margin: 0px;
    box-sizing: border-box;
}

.frame {
	max-width: 1300px;
    padding: 0 30px;
    margin: 0 auto;
}

/*//common css*/

/*main css*/
.row{
	
	max-width: 1300px;
    padding: 10 40px;
    margin: 10 auto;
	display: flex;
	justify-content: space-between;
	height: 120px;

}

.select-group {
	max-width: 1300px;
    padding: 0 30px;
    margin: 0 auto;
	display: flex;
	justify-content: space-between;
	height: 120px;
}

/*.select-group {
	width: 200px;
	height: 60px;
}*/


#main_post {
	min-height: 1000px;
	height: auto;
}

.main_page_title {
	text-align: center;
	width: 100%;
	height: auto;
	padding-top: 40px;
	margin-bottom: 30px;
}

.main_page_title_word {
	font-weight: 700;
	font-size: 24px;
}

.main_item_box {
	width: 100%;
	height: auto;
}

.main_post_list {
	display: grid;
	grid-template-columns: repeat(5, 1fr);
	grid-gap: 10px;
	grid-auto-rows: minmax(100px, auto);
}

.main_post_box {
	display: relative;
	height: auto;
}

.link_post {
	position: absolute;
	display: block;
	width: 240px;
	height: 335px;
}

.main_post_item {
	padding-bottom: 30px;
	list-style: none;
	text-decoration: none;
}

.main_post_item_img {
	width: 100%;
	height: 224px;
}

.MemberName {
	font-size: 20px;
	font-weight: 1000;
}

.cateName {
	font-size: 20px;
	font-weight: 1000;
}

.title {
	display: inline-block;
	font-size: 17px;
	font-weight: 700;
	max-width: 240px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.readCnt {
	color: #757575;
	font-size: 14px;
}
/*//main css*/

/*footer css*/
footer {
	bottom: 0;
}

.footer-black {
	margin-top: 100px;
	padding: 20px 0px;
	height: 70px;
	background-color: #1e1e1e;
	display: block;
}

.footer-text {
	color: #e2e2e2;
	font-size: 13px; 
	opacity: 0.56;
	font-weight: 500;
}


/*//footer css*/